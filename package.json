{
  "name": "electron-react-app",
  "version": "0.1.0",
  "private": true,
  "DEV": true,
  "author": "lifan",
  "description": "electron-react-app",
  "main": "./public/electron.js",
  "homepage": "./",
  "dependencies": {
    "electron": "^3.0.8",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-start": "./node_modules/.bin/electron .",
    "dist": "npm run build && rm -rf dist && electron-builder"
  },
  "devDependencies": {
    "electron-builder": "^20.10.0",
    "electron-load-devtool": "^1.0.0"
  },
  "build": {
    "appId": "com.isoftstone.apptools",
    "productName": "Electron React",
    "dmg": {
      "icon": "./public/icon/icon.icns"
    },
    "mac": {
      "icon": "./public/icon/icon.icns"
    },
    "win": {
      "icon": "./public/icon/icon.ico"
    }
