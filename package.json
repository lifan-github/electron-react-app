{
  "name": "electron-react-app",
  "version": "0.1.0",
  "private": true,
  "DEV": false,
  "author": "lifan",
  "description": "electron-react-app",
  "keywords": [
    "electron",
    "react",
    "antd"
  ],
  "main": "./public/electron.js",
  "homepage": "./",
  "dependencies": {
    "antd": "^3.4.3",
    "babel-plugin-import": "^1.7.0",
    "history": "^4.7.2",
    "mockjs": "^1.0.1-beta3",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-start": "./node_modules/.bin/electron .",
    "pack": "electron-builder --dir",
    "dist": "npm run build && rm -rf dist && electron-builder",
    "dist:winx86": "electron-builder --platform win32 --arch ia32",
    "dist:winx64": "electron-builder --platform win32 --arch x64"
  },
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-builder": "^20.10.0",
    "electron-load-devtool": "^1.0.0"
  },
  "build": {
    "appId": "com.isoftstone.apptools",
    "productName": "Electron React",
    "mac": {
      "icon": "build/icon/icon.icns"
    },
    "win": {
      "icon": "build/icon/icon.ico"
    },
    "linux": {
      "icon": "build/icon/icon.ico"
    }
  }
}
